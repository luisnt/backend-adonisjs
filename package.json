{
  "name": "e-svt",
  "version": "1.0.0",
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "private": true,
  "scripts": {
    "deploy": "cd build && npm install --save-dev @adonisjs/assembler && npm ci --production",
    "db": "node ace migration:run",
    "build": "node ace build --production --ignore-ts-errors",
    "start": "ENV_SILENT=true node server.js",
    "dev": "node ace serve --watch",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/nanoid": "^3.0.0",
    "adonis-preset-ts": "^2.1.0",
    "adonisjs-types": "^1.3.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.2",
    "eslint-plugin-prettier": "^3.4.0",
    "pino-pretty": "^5.1.1",
    "prettier": "^2.3.2",
    "typescript": "^4.2.4",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/assembler": "^5.3.5",
    "@adonisjs/ace": "^11.0.3",
    "@adonisjs/ally": "^4.1.0",
    "@adonisjs/application": "^5.1.5",
    "@adonisjs/auth": "^8.0.7",
    "@adonisjs/core": "^5.1.11",
    "@adonisjs/lucid": "^15.0.3",
    "@adonisjs/mail": "^7.2.3",
    "@adonisjs/repl": "^3.1.4",
    "@adonisjs/session": "^6.1.0",
    "@adonisjs/view": "^6.0.6",
    "@types/node": "^16.6.1",
    "luxon": "^2.0.1",
    "nanoid": "^3.1.23",
    "pg": "^8.7.1",
    "phc-argon2": "^1.1.2",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
